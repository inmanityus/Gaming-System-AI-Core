  pages: [],
  forms: [],
  links: [],
  emails: [],
  errors: []
};

// Test ALL pages
for (const page of allPages) {
  await navigateTo(page);
  await verifyPageLoads();
  await verifyAllLinksOnPage();
  await verifyAllButtonsOnPage();
  testResults.pages.push({page, status: 'passed'});
}

// Test ALL forms
for (const form of allForms) {
  await testFormValidation(form);
  await testFormSubmission(form, validData);
  await testFormSubmission(form, invalidData);
  await verifyFormEmail(form);
  testResults.forms.push({form, status: 'passed'});
}

// Verify NO errors
if (testResults.errors.length > 0) {
  // FIX ALL ERRORS before proceeding
  await fixAllErrors(testResults.errors);
  // RE-RUN tests until ALL pass
  await rerunAllTests();
}
```

