{
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "storyteller",
      "image": "695353648052.dkr.ecr.us-east-1.amazonaws.com/bodybroker-services:storyteller-latest",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "us-east-1",
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/gaming-system/storyteller",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "containerPort": 8006,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "value": "storyteller",
          "name": "SERVICE_NAME"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::695353648052:role/ecsTaskExecutionRole",
  "family": "storyteller"
}
